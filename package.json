{
  "name": "tcp-udp-router",
  "version": "1.0.0",
  "description": "Professional TCP/UDP Router with middleware, plugin system, and metrics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:e2e": "jest test/e2e",
    "test:performance": "jest test/performance",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint src/**/*.ts test/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "check:all": "npm run lint && npm run type-check && npm test",
    "check:ports": "node scripts/check-ports.js",
    "check:config": "node scripts/validate-config.js",
    "validate:plugins": "node scripts/validate-plugins.js",
    "validate:plugin": "node scripts/validate-single-plugin.js",
    "check:dependencies": "npm audit",
    "reinstall:plugins": "node scripts/reinstall-plugins.js",
    "diagnose": "node scripts/diagnose.js",
    "docs:dev": "docsify serve docs",
    "docs:build": "node scripts/build-docs.js",
    "docs:serve": "http-server docs -p 3000 -o",
    "docs:validate": "node scripts/validate-docs.js",
    "docs:spellcheck": "cspell \"docs/**/*.md\"",
    "docs:linkcheck": "markdown-link-check docs/**/*.md",
    "clean": "rm -rf dist coverage .nyc_output",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "docker:build": "docker build -t tcp-udp-router .",
    "docker:run": "docker run -p 4000:4000 -p 5000:5000 -p 3001:3001 -p 8080:8080 tcp-udp-router",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "k8s:logs": "kubectl logs -f deployment/tcp-udp-router",
    "k8s:port-forward": "kubectl port-forward svc/tcp-udp-router 4000:4000",
    "benchmark": "node scripts/benchmark.js",
    "profile": "node --prof scripts/profile.js",
    "profile:analyze": "node --prof-process isolate-*.log > profile.txt",
    "security:audit": "npm audit && snyk test",
    "security:fix": "npm audit fix",
    "release:patch": "npm version patch && npm run build && npm publish",
    "release:minor": "npm version minor && npm run build && npm publish",
    "release:major": "npm version major && npm run build && npm publish",
    "prepublishOnly": "npm run check:all && npm run build",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "tcp",
    "udp",
    "router",
    "network",
    "proxy",
    "middleware",
    "plugins",
    "metrics",
    "monitoring",
    "typescript",
    "nodejs"
  ],
  "author": "TCP/UDP Router Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tcp-udp-router.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/tcp-udp-router/issues"
  },
  "homepage": "https://github.com/your-org/tcp-udp-router#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@types/node": "^24.3.0",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "prom-client": "^15.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "winston": "^3.14.2",
    "zod": "^4.1.5",
    "crypto": "^1.0.1",
    "net": "^1.0.2",
    "dgram": "^1.0.1",
    "events": "^3.3.0",
    "util": "^0.12.5",
    "path": "^0.12.7",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "cluster": "^0.7.7",
    "worker_threads": "^1.0.0",
    "zlib": "^1.0.5",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "url": "^0.11.3",
    "querystring": "^0.2.1",
    "stream": "^0.0.2",
    "buffer": "^6.0.3",
    "assert": "^2.1.0",
    "timers": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.14.2",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.5.0",
    "mocha": "^10.7.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "standard-version": "^9.5.0",
    "docsify": "^4.4.6",
    "docsify-cli": "^4.4.4",
    "http-server": "^14.1.1",
    "cspell": "^7.3.7",
    "markdown-link-check": "^3.11.3",
    "markdownlint": "^0.37.0",
    "markdownlint-cli": "^0.33.0",
    "snyk": "^1.1234.0",
    "autocannon": "^7.12.0",
    "artillery": "^2.0.0",
    "k6": "^0.45.0",
    "docker": "^1.0.0",
    "docker-compose": "^1.0.0",
    "kubectl": "^1.0.0",
    "helm": "^3.12.0",
    "prometheus": "^2.45.0",
    "grafana": "^10.0.0",
    "jaeger": "^1.47.0",
    "zipkin": "^0.22.0",
    "opentelemetry": "^1.18.0",
    "opentelemetry-node": "^0.39.0",
    "opentelemetry-exporter-jaeger": "^1.18.0",
    "opentelemetry-instrumentation": "^0.39.0",
    "opentelemetry-sdk-node": "^0.39.0"
  },
  "peerDependencies": {
    "node": ">=16.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "files": [
    "dist/**/*",
    "docs/**/*",
    "examples/**/*",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "lib": "dist",
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  }
}
